{#
/**
 * @file
 * Custom exposed form for Advanced Search view.
 *
 * View: advanced_search
 * Display: page_1
 */
#}

<form{{ attributes }}>
  <div class="d-flex flex-row">
    <div class="row g-3">
      <div class="col-12 col-md-6 col-lg-3">
        {% for key, item in form %}
          {% if key starts with '#' %}
          {% else %}
              {{ item }}
          {% endif %}
        {% endfor %}
      </div>
    </div>
  </div>
</form>
