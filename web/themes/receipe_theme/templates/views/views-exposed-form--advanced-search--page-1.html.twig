{#
/**
 * @file
 * Custom exposed form for Advanced Search view.
 *
 * View: advanced_search
 * Display: page_1
 */
#}

<form{{ attributes }}>
    <div class="row g-3">
        {% for key, item in form %}
            {% if key starts with '#' %}
            {% else %}
            <div class="col-12 col-md-4 col-lg-4">
                {{ item }}
            </div>
            {% endif %}
        {% endfor %}
    </div>
</form>
